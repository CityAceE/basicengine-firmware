10 REM 'OUT' SEARCH PROGRAM
20 FOR P=200 TO 255
30 FOR B=0 TO 5
40 PRINT "PORT #";P
50 PRINT "BYTE #";B
60 CALL OUT(P,B)
70 NEXT B
80 NEXT P
99 END
65000 REM FAKE OUT()
65010 PROC OUT(X,Y)
65020   REM SIMULATE CASSETTE MOTOR
65030   IF @X <> 255 THEN 65110
65040   REM @X = 255
65050   IF @Y AND 4 THEN 65090
65060   REM BIT OFF
65070   PRINT "MOTOR OFF"
65080   GOTO 65110
65090   REM BIT ON
65100   PRINT "MOTOR ON"
65110 RETURN
