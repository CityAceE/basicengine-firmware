5 DIM M(100)
10 REM 'FETCH' TEST PROGRAM
20 Y=1
30 FOR X=18368 TO 18377
40 CALL STUFF(X,Y)
50 Y=Y+1
60 NEXT X
70 FOR X=18368 TO 18377
80 Y=FN FETCH(X)
90 PRINT " ";Y;" ";
100 NEXT X
110 PRINT
120 PRINT "'FETCH' PASSED THE TEST IF #1 THRU #10 ARE PRINTED"
999 END
65000 REM FAKE STUFF() & FETCH()
65010 PROC STUFF(X,Y)
65020   M(X-18300)=Y
65030 RETURN
65040 PROC FETCH(X)
65050   RETURN M(@X-18300)
