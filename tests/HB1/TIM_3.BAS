10 REM 'TIM' TEST PROGRAM
11 GOTO 20
15 PROC TIM:RETURN TICK()
20 PRINT "TIME IS MARCHING ON"
30 A=FN TIM()
40 REM FOR X=1 TO 2000
45 WAIT 3
50 REM NEXT X
60 B=FN TIM()
70 IF B > A THEN 100
80 PRINT "THE 'TIM' FUNCTION FAILED THE TEST"
90 GOTO 999
100 PRINT "'TIM' PASSED THE TEST -- ELAPSED TIME=";B-A
999 END
