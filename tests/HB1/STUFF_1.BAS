5 DIM M(100)
10 REM 'STUFF' TEST PROGRAM
20 FOR X=1 TO 10
30 CALL STUFF(15000+X,X)
40 NEXT X
50 FOR X=15001 TO 15010
60 Y=FN FETCH(X)
70 PRINT Y;
80 NEXT X
90 PRINT
100 PRINT "'STUFF' PASSED THE TEST IF #1 THRU #10 ARE PRINTED"
999 END
65000 REM FAKE STUFF() & FETCH()
65010 PROC STUFF(X,Y)
65020   M( @X - 15000 ) = @Y
65030   RETURN
65040 PROC FETCH(X)
65050   RETURN M( @X - 15000 )
